<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>KeyPayV2</RootNamespace>
    <AssemblyName>KeyPayV2</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>2.0.1.27</AssemblyVersion>
    <FileVersion>2.0.1.27</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/keypay/keypay-dotnet</PackageProjectUrl>
    <PackageIcon>keypay.png</PackageIcon>
    <RepositoryUrl>https://github.com/keypay/keypay-dotnet</RepositoryUrl>
    <PackageTags>keypay yourpayroll api payroll</PackageTags>
    <PackageReleaseNotes>Currently in beta and not supported. Use at your own risk.
This is a major version bump. This new version of the client is automatically generated from our source APIs meaning that it can always be kept up to date.
This new version also provides proper support for multiple regions.
This release: Job  build 26 JSON updates</PackageReleaseNotes>
    <Copyright>Copyright 2020</Copyright>
    <Description>A .NET library for the KeyPay API</Description>
    <Version>2.0.1.27</Version>
  </PropertyGroup>
  <ItemGroup>
    <None Include="keypay.png" Pack="true" PackagePath="\"/>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.1" />
    <PackageReference Include="RestSharp" Version="106.10.1" />
  </ItemGroup>

</Project>
