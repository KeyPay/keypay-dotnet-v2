<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<RootNamespace>KeyPayV2</RootNamespace>
		<AssemblyName>KeyPayV2</AssemblyName>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>2.0.2.575</AssemblyVersion>
    <FileVersion>2.0.2.575</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/keypay/keypay-dotnet</PackageProjectUrl>
    <PackageIcon>keypay.png</PackageIcon>
    <RepositoryUrl>https://github.com/keypay/keypay-dotnet</RepositoryUrl>
    <PackageTags>keypay yourpayroll api payroll</PackageTags>
    <PackageReleaseNotes>Provides proper support for multiple regions.
This release: Update CodeGenerator.cs</PackageReleaseNotes>
    <Copyright>Copyright 2022</Copyright>
    <Description>A .NET library for the KeyPay API</Description>
    <Version>2.0.2.575</Version>
  </PropertyGroup>
  <ItemGroup>
    <None Include="keypay.png" Pack="true" PackagePath="\"/>
  </ItemGroup>

	<ItemGroup>
		<PackageReference Include="RestSharp" Version="108.0.1" />
		<PackageReference Include="RestSharp.Serializers.NewtonsoftJson" Version="108.0.1" />
	</ItemGroup>

</Project>
