<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>KeyPayV2</RootNamespace>
    <AssemblyName>KeyPayV2</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>2.0.0.140</AssemblyVersion>
    <FileVersion>2.0.0.140</FileVersion>
    <PackageLicenseUrl>http://opensource.org/licenses/mit-license</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/keypay/keypay-dotnet</PackageProjectUrl>
    <PackageIconUrl>http://keypay.com.au/wp-content/uploads/2016/07/home-banner-icon.svg</PackageIconUrl>
    <RepositoryUrl>https://github.com/keypay/keypay-dotnet</RepositoryUrl>
    <PackageTags>keypay yourpayroll api payroll</PackageTags>
    <PackageReleaseNotes>This is a major version bump. This new version of the client is automatically generated from our source APIs meaning that it can always be kept up to date.
    This new version also provides proper support for multiple regions.
    This release: Remove branching on keypay-dev</PackageReleaseNotes>
    <Copyright>Copyright 2020</Copyright>
    <Description>A .NET library for the KeyPay API</Description>
    <Version>2.0.0.140</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.1" />
    <PackageReference Include="RestSharp" Version="106.10.1" />
  </ItemGroup>

</Project>
